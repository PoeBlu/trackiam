{
  "Arn": "arn:aws:iam::aws:policy/aws-service-role/AWSServiceRoleForImageBuilder",
  "CreateDate": "2019-11-29T22:02:13Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/aws-service-role/",
  "PolicyId": "ANPAZKAPJZG4NE22WISEW",
  "PolicyName": "AWSServiceRoleForImageBuilder",
  "PolicyVersion": {
    "CreateDate": "2019-11-29T22:02:13Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": "autoscaling.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "autoscaling:CreateAutoScalingGroup"
          ],
          "Condition": {
            "StringEquals": {
              "aws:RequestTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "autoscaling:DeleteAutoScalingGroup",
            "autoscaling:DetachInstances"
          ],
          "Condition": {
            "StringEquals": {
              "autoscaling:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DeleteLaunchTemplate",
            "ec2:StopInstances",
            "ec2:TerminateInstances"
          ],
          "Condition": {
            "StringEquals": {
              "ec2:ResourceTag/CreatedBy": "EC2 Image Builder"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:SendCommand"
          ],
          "Condition": {
            "StringLike": {
              "ssm:resourceTag/CreatedBy": [
                "EC2 Image Builder"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*:*:instance/*"
          ]
        },
        {
          "Action": "ec2:CreateTags",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ec2:*::image/*"
          ]
        },
        {
          "Action": "ssm:SendCommand",
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:*:*:document/AWS-RunPowerShellScript",
            "arn:aws:ssm:*:*:document/AWS-RunShellScript"
          ]
        },
        {
          "Action": "ssm:StartAutomationExecution",
          "Effect": "Allow",
          "Resource": "arn:aws:ssm:*:*:automation-definition/ImageBuilder*"
        },
        {
          "Action": [
            "autoscaling:DescribeAutoScalingGroups",
            "autoscaling:SuspendProcesses"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CopyImage",
            "ec2:CreateImage",
            "ec2:CreateLaunchTemplate",
            "ec2:DeregisterImage",
            "ec2:DescribeImages",
            "ec2:DescribeInstanceAttribute",
            "ec2:DescribeInstanceStatus",
            "ec2:DescribeInstances",
            "ec2:DescribeSubnets",
            "ec2:DescribeTags",
            "ec2:ModifyImageAttribute"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "license-manager:UpdateLicenseSpecificationsForResource"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "sns:Publish"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ssm:AddTagsToResource",
            "ssm:DescribeInstanceInformation",
            "ssm:GetAutomationExecution",
            "ssm:ListCommandInvocations",
            "ssm:ListCommands",
            "ssm:StopAutomationExecution"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2019-11-29T22:02:13Z"
}
