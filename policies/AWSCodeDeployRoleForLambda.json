{
  "Arn": "arn:aws:iam::aws:policy/service-role/AWSCodeDeployRoleForLambda",
  "CreateDate": "2017-11-28T14:05:44Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/service-role/",
  "PolicyId": "ANPAJA3RQZIKNOSJ4ZQSA",
  "PolicyName": "AWSCodeDeployRoleForLambda",
  "PolicyVersion": {
    "CreateDate": "2017-12-01T22:32:58Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "s3:GetObject",
            "s3:GetObjectVersion"
          ],
          "Condition": {
            "StringEquals": {
              "s3:ExistingObjectTag/UseWithCodeDeploy": "true"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudwatch:DescribeAlarms",
            "lambda:GetAlias",
            "lambda:UpdateAlias",
            "sns:Publish"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "lambda:InvokeFunction"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:lambda:*:*:function:CodeDeployHook_*"
        },
        {
          "Action": [
            "s3:GetObject",
            "s3:GetObjectVersion"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:s3:::*/CodeDeploy/*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2017-12-01T22:32:58Z"
}
