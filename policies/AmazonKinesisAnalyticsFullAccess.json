{
  "Arn": "arn:aws:iam::aws:policy/AmazonKinesisAnalyticsFullAccess",
  "CreateDate": "2016-09-21T19:01:14Z",
  "DefaultVersionId": "v1",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJQOSKHTXP43R7P5AC",
  "PolicyName": "AmazonKinesisAnalyticsFullAccess",
  "PolicyVersion": {
    "CreateDate": "2016-09-21T19:01:14Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:PassRole",
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/service-role/kinesis-analytics*"
        },
        {
          "Action": "kinesisanalytics:*",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "logs:GetLogEvents",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudwatch:GetMetricStatistics",
            "cloudwatch:ListMetrics"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "firehose:DescribeDeliveryStream",
            "firehose:ListDeliveryStreams"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "iam:ListPolicyVersions",
            "iam:ListRoles"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "kinesis:CreateStream",
            "kinesis:DeleteStream",
            "kinesis:DescribeStream",
            "kinesis:ListStreams",
            "kinesis:PutRecord",
            "kinesis:PutRecords"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v1"
  },
  "UpdateDate": "2016-09-21T19:01:14Z"
