          "Resource": "arn:aws:mgh:*:*:progressUpdateStream/DMS/*"
        },
        {
          "Action": [
            "mgh:CreateProgressUpdateStream"
          ],
          "Effect": "Allow",
          "Resource": "arn:aws:mgh:*:*:progressUpdateStream/DMS"
        },
        {
          "Action": [
            "mgh:GetHomeRegion",
            "mgh:ListMigrationTasks"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
