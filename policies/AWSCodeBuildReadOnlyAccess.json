{
  "Arn": "arn:aws:iam::aws:policy/AWSCodeBuildReadOnlyAccess",
  "CreateDate": "2016-12-01T19:03:41Z",
  "DefaultVersionId": "v6",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJIZZWN6557F5HVP2K",
  "PolicyName": "AWSCodeBuildReadOnlyAccess",
  "PolicyVersion": {
    "CreateDate": "2019-11-05T22:11:42Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "codestar-notifications:DescribeNotificationRule"
          ],
          "Condition": {
            "StringLike": {
              "codestar-notifications:NotificationsForResource": "arn:aws:codebuild:*"
            }
          },
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsPowerUserAccess"
        },
        {
          "Action": [
            "cloudwatch:GetMetricStatistics",
            "codebuild:BatchGet*",
            "codebuild:List*",
            "codecommit:GetBranch",
            "codecommit:GetCommit",
            "codecommit:GetRepository",
            "events:DescribeRule",
            "events:ListRuleNamesByTarget",
            "events:ListTargetsByRule",
            "logs:GetLogEvents"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "codestar-notifications:ListEventTypes",
            "codestar-notifications:ListNotificationRules"
          ],
          "Effect": "Allow",
          "Resource": "*",
          "Sid": "CodeStarNotificationsListAccess"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v6"
  },
  "UpdateDate": "2019-11-05T22:11:42Z"
}
